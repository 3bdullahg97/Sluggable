{
    "name": "luqta/sluggable",
    "description": "Sluggable trait package supports english and arabic slugs",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdullah Abdelqader",
            "email": "3bdullahg97@gmail.com",
            "homepage": "https:://twitter.com/3bdullahg97"
        }
    ],
    "homepage": "https://github.com/3bdullahg97/sluggable",
    "keywords": ["Laravel", "Sluggable"],
    "require": {
        "illuminate/support": "~5|~6",
        "jenssegers/mongodb": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Luqta\\Sluggable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luqta\\Sluggable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luqta\\Sluggable\\SluggableServiceProvider"
            ],
            "aliases": {
                "Sluggable": "Luqta\\Sluggable\\Facades\\Sluggable"
            }
        }
    }
}
